<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns		= "http://www.w3.org/ns/widgets"
	xmlns:gap	= "http://phonegap.com/ns/1.0"
	id		= "com.phonegap.getting.started"
	version 	= "1.0.0">

	<name>Eyelander</name>

	<description>
		Demo app for concept designed by MirkoLazovic.com. Landing eyes on the right places:  location aware, motion aware...
	</description>

	<author href="http://marnixdekker.github.com"
		email="dekker.marnix@gmail.com">
		Marnix
	</author>

	<icon src="icon.png" gap:role="default" />

	<feature name="http://api.phonegap.com/1.0/geolocation"/>
	<feature name="http://api.phonegap.com/1.0/network"/>

	<preference name="orientation" value="portrait" />
</widget>
